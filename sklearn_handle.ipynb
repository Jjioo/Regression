        "from sklearn.datasets import load_iris\n",
        "\n",
        "# Load the Iris dataset\n",
        "data = load_iris()\n",
        "\n",
        "# Extract the input features (X) and the labels (y)\n",
        "X = data.data\n",
        "y = data.target\n",
        "\n",
        "# Print the shape of X and y\n",
        "print(\"Shape of X:\", X.shape)\n",
        "print(\"Shape of y:\", y.shape)\n"
